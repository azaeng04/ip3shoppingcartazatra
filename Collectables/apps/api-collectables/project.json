{
  "name": "api-collectables",
  "projectType": "application",
  "root": "apps/api-collectables",
  "sourceRoot": "apps/api-collectables",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "php -S 127.0.0.1:8000 -t apps/api-collectables",
        "cwd": ".",
        "color": true
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "find apps/api-collectables -name '*.php' -print0 | xargs -0 -n1 php -l",
        "cwd": "."
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "php bin/composer test -- --testsuite ApiCollectables",
        "cwd": ".",
        "color": true
      }
    }
  },
  "tags": []
}
